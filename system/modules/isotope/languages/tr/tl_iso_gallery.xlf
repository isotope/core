      <trans-unit id="tl_iso_gallery.lightbox_legend">
        <source>Lightbox/Mediabox</source>
        <target>Lightbox/Mediabox</target>
      </trans-unit>
      <trans-unit id="tl_iso_gallery.template_legend">
        <source>Template settings</source>
        <target>Şablon ayarları</target>
      </trans-unit>
      <trans-unit id="tl_iso_gallery.none">
        <source>No link action</source>
        <target>Link eylemi yok</target>
      </trans-unit>
      <trans-unit id="tl_iso_gallery.reader">
        <source>Link to the product reader</source>
        <target>Ürün okuyucusuna bağla</target>
      </trans-unit>
      <trans-unit id="tl_iso_gallery.lightbox">
        <source>Open lightbox/mediabox</source>
        <target>Lightbox/mediabox aç</target>
      </trans-unit>
      <trans-unit id="tl_iso_gallery.includeJQuery">
        <source>Make sure to include jQuery in your page layout for this gallery to work correctly.</source>
        <target>Bu galerinin düzgün çalışması için sayfa düzeninize jQuery eklediğinizden emin olun.</target>
      </trans-unit>
