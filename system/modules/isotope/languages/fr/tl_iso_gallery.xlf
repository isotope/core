        <target>Sélectionner la position où le filigrane sera appliqué.</target>
      </trans-unit>
      <trans-unit id="tl_iso_gallery.gallery_watermark_image.0">
        <source>Gallery watermark image</source>
        <target>Filigrane de la galerie</target>
