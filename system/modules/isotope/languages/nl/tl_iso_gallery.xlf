        <source>Name</source>
        <target>Naam</target>
      </trans-unit>
      <trans-unit id="tl_iso_gallery.name.1">
        <source>Enter a name for this gallery.</source>
        <target>Vul een naam in voor deze gallerij.</target>
      </trans-unit>
      <trans-unit id="tl_iso_gallery.type.0">
        <source>Type</source>
        <target>Soort</target>
      </trans-unit>
