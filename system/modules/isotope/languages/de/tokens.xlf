        <target>Versandart-Hinweis.</target>
      </trans-unit>
      <trans-unit id="NOTIFICATION_CENTER_TOKEN.iso_order_status_change.config_*">
        <source>All the store configuration fields.</source>
        <target>Alle Shop-Konfigurations-Felder.</target>
      </trans-unit>
      <trans-unit id="NOTIFICATION_CENTER_TOKEN.iso_order_status_change.member_*">
